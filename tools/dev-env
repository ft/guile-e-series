GUILE_AUTO_COMPILE=0
GUILE_WARN_DEPRECATED=detailed
GUILE_BINARY="${GUILE_BINARY:-guile}"
GUILE_LOAD_PATH="$PWD/scheme${GUILE_LOAD_PATH:+:}$GUILE_LOAD_PATH"
GUILE_LOAD_COMPILED_PATH="$PWD/scheme${GUILE_COMPILED_LOAD_PATH:+:}$GUILE_COMPILED_LOAD_PATH"
GUILE_AUTO_COMPILE=0

for i in GUILE_WARN_DEPRECATED GUILE_LOAD_PATH GUILE_LOAD_COMPILED_PATH GUILE_AUTO_COMPILE; do
    export "$i"
done
unset i
